{
  "name": "gatsby-starter-default",
  "version": "0.1.0",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "keywords": [
    "gatsby"
  ],
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "license": "MIT",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "easy-scripts format",
    "lint": "easy-scripts lint",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "validate": "easy-scripts validate"
  },
  "husky": {
    "hooks": {
      "commit-msg": "easy-scripts commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "easy-scripts pre-commit",
      "post-rewrite": "easy-scripts pre-commit"
    }
  },
  "babel": {
    "presets": [
      "./node_modules/easy-scripts/dist/config/babelrc.js"
    ]
  },
  "eslintConfig": {
    "extends": "./node_modules/easy-scripts/dist/config/eslintrc.js",
    "rules": {
      "react/prop-types": "off"
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "preset": "./node_modules/easy-scripts/dist/config/jest.config.js",
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es)"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.4.0",
    "@types/styled-components": "^4.1.12",
    "gatsby": "^2.2.3",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-sharp": "^2.0.30",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-source-filesystem": "^2.0.27",
    "gatsby-transformer-sharp": "^2.1.17",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "easy-scripts": "^2.3.12",
    "react-testing-library": "^6.0.0",
    "typescript": "^3.3.3"
  }
}
